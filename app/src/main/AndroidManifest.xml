<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smile">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/carregar"
        android:label="@string/app_name"
        android:roundIcon="@drawable/carregar"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ReciveBluetooth"></activity>
        <activity android:name=".Bluetooth" />
        <activity android:name=".Listar" />
        <activity android:name=".Adiciona_actv" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>